{
  "manifest_version": 3,
  "browser_specific_settings": {
    "gecko": {
      "id": "fb-getridadd@edvard.cz"
    }
  },
  "content_scripts": [
    {
      "js": [
        "jquery-3.7.1.min.js",
        "launcher.js"
      ],
      "run_at": "document_end",
      "matches": [
        "http://*.facebook.com/*",
        "https://*.facebook.com/*"
      ]
    }
  ],
  "version": "0.3.1",
  "name": "fb-getridad",
  "description": "Release the wall from imposed content",
  "icons": {
    "16": "48x48.png"
  }
}