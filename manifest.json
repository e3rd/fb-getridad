{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "launcher.js"
      ],
      "run_at": "document_end",
      "matches": [
        "https://*.facebook.com/*"
      ]
    }
  ],
  "version": "0.2",
  "name": "Facebook getridad",
  "description": "Make facebook a better place",
  "icons": {
    "48": "48x48.png"
  }
}
