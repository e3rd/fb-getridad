{
  "manifest_version": 2,
  "browser_specific_settings": {
    "gecko": {
      "id": "fb-getridadd@edvard.cz"
    }
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "launcher.js"
      ],
      "run_at": "document_end",
      "matches": [
        "https://*.facebook.com/*"
      ]
    }
  ],
  "version": "0.2.3",
  "name": "fb-getridad",
  "description": "Release the wall from imposed content",
  "icons": {
    "16": "48x48.png"
  }
}
